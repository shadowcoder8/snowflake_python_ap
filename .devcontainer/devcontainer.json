{
	"name": "Snowflake API Dev",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "api",
	"workspaceFolder": "/app",
	"initializeCommand": "touch .env",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-azuretools.vscode-docker"
			]
		}
	},
	"forwardPorts": [8000],
	"portsAttributes": {
		"8000": {
			"label": "Snowflake API",
			"onAutoForward": "notify"
		}
	},
	"remoteUser": "root"
}
